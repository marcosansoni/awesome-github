{
  "name": "awesome-github",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "generate": "graphql-codegen --config codegen.yml -r dotenv/config",
    "postinstall": "remix setup node",
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "NODE_ENV=development remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "test": "vitest",
    "typecheck": "tsc -b",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@apollo/client": "^3.6.6",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@remix-run/node": "^1.4.0",
    "@remix-run/react": "^1.4.0",
    "@remix-run/vercel": "^1.4.0",
    "clsx": "^1.1.1",
    "downshift": "^6.1.7",
    "graphql": "^16.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.15.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typed-document-node": "^2.2.13",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@remix-run/dev": "^1.4.0",
    "@remix-run/eslint-config": "^1.4.0",
    "@remix-run/serve": "^1.4.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.9",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.0.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-modal": "^3.13.1",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.12.0",
    "eslint-config-kentcdodds": "^20.2.0",
    "happy-dom": "^2.55.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "react-router-dom": "^6.3.0",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.23",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.1",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.8.2"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{js,css,md,ts,tsx}": "prettier --write"
  }
}
